{
  "defaults": {
    "cascade_size_limits": [10, 25, 50, 100],
    "n_experiments_per_p": 100,
    "seeds": {
      "graph": 42,
      "patient_zero_base_seed": 20,
      "model_base_seed": 67
    },
    "models": {
      "IC": {
        "type": "IC",
        "params": {
          "p_values": {
            "start": 0.3,
            "stop": 1.0,
            "num": 100
          }
        }
      },
      "SIR": {
        "type": "SIR",
        "params": {
          "p_values": {
            "start": 0.01,
            "stop": 1.0,
            "num": 100
          },
          "p_recover": 0.2
        }
      }
    }
  },

  "simulations": [
    {
      "name": "k_regular",
      "graph": {
        "type": "k_regular",
        "params": { "degree": 3, "nodes": 1000 }
      },
      "models": ["IC", "SIR"]
    },
    {
      "name": "watts_strogatz",
      "graph": {
        "type": "watts_strogatz",
        "params": { "nodes": 1000, "neighbors": 10, "probability": 0.2 }
      },
      "models": ["IC", "SIR"]
    },
    {
      "name": "erdos_renyi",
      "graph": {
        "type": "erdos_renyi",
        "params": { "nodes": 1000, "probability": 0.2 }
      },
      "models": ["IC", "SIR"]
    },
    {
      "name": "barabasi_albert",
      "graph": {
        "type": "barabasi_albert",
        "params": { "nodes": 1000, "edges": 3 }
      },
      "models": ["IC", "SIR"]
    }
  ]
}
